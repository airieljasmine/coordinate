 * Generated by Eqela Studio 2.0b7.4
 */

public class MainScene : SEScene
{
		SESprite text;
		int w;
		int h;
		int wt;
		int ht;
		
	public void initialize(SEResourceCache rsc) {
		base.initialize(rsc);
		h = get_scene_height();
		w = get_scene_width();
		add_entity(SESpriteEntity.for_color(Color.instance("white"), get_scene_width(), get_scene_height()));
		rsc.prepare_font("myfont","arial bold color=black",40);
		text = add_sprite_for_text("JASMINE","myfont");
		wt = text.get_width();
		ht = text.get_height();
		text.move(0, h-ht);
	}

public void on_pointer_press(SEPointerInfo pi) {
	base.on_pointer_press(pi);
	text.set_text("on_pointer_press");
}	

public void on_pointer_release(SEPointerInfo pi) {
	base.on_pointer_release(pi);
	text.set_text("on_pointer_release");
}	

public void on_key_press(String name, String str) {
	base.on_key_press(name, str);
	text.set_text("PALQUEZA");
}	





	public void cleanup() {
		base.cleanup();
	}
}
